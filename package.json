{
  "name": "logseq-maps-plugin",
  "author": "benjypng",
  "description": "Create maps as part of your notes.",
  "license": "MIT",
  "logseq": {
    "id": "logseq-maps-plugin",
    "title": "logseq-maps-plugin",
    "icon": "./icon.svg",
    "main": "dist/index.html"
  },
  "scripts": {
    "dev": "npx vite",
    "build": "npx eslint . --fix && npx tsc && npx vite build",
    "preview": "npx vite preview",
    "prepare": "husky"
  },
  "release": {
    "branches": [
      "main"
    ],
    "plugins": [
      [
        "@semantic-release/github",
        {
          "assets": [
            "logseq-maps-plugin.zip"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "@logseq/libs": "^0.2.10",
    "@react-leaflet/core": "^3.0.0",
    "axios": "^1.13.2",
    "leaflet": "^1.9.4",
    "leaflet-routing-machine": "^3.2.12",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.69.0",
    "react-leaflet": "^5.0.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/eslint": "^9.6.1",
    "@types/eslint-config-prettier": "^6.11.3",
    "@types/leaflet": "^1.9.21",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@typescript-eslint/eslint-plugin": "^8.50.1",
    "@typescript-eslint/parser": "^8.50.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react-you-might-not-need-an-effect": "^0.8.2",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "husky": "^9.1.7",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.50.1",
    "vite": "^7.3.0",
    "vite-plugin-logseq": "^1.1.2",
    "vite-plugin-static-copy": "^3.1.4",
    "vite-tsconfig-paths": "^6.0.3"
  }
}
